public class Demo {
         
  public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int i = scanner.nextInt();
        // i = 1;
        for(int u = 0; u < i; u++){
                int o = scanner.nextInt();
                long num[] = new long[o];
                for(int x = 0; x < o; x++){
                        num[x] = scanner.nextLong();
                }
                int g = 0; 
                int p = 0;
                while(true){
                        if(num.length == 1){
                                long sum = num[0];
                                while(true){
                                        
                                        if(sum%2 == 0){
                                                p++;
                                                System.out.println(g);
                                                break;
                                        }
                                        g++;
                                        sum = sum/2;
                                }
                        } else {
                                
                        long z = 0;
                        for(int x = 0; x < o; x++){
                                long sum = 0;
                                for(int x1 = 0; x1 < o; x1++){
                                        sum = sum + num[x1];
                                }
                                if(g == 0){
                                         sum = (sum - num[x])+num[x];
                                } else {
                                        z = num[x];
                                        for(int v =0; v < g; v++){
                                                z = z/2;
                                        }
                                         sum = (sum - num[x])+z;
                                }
                               
                                if(sum%2 == 0){
                                        System.out.println(g);
                                        p++;
                                        break;
                                }
                               
                        }
                        
                }
                if(p == 0){
                                g++;
                        } else {
                                break;
                        }
                        }
        }
}
